{
  "name": "nodejsmq",
  "version": "0.1.7",
  "description": "A simple rabbitmq implementation",
  "main": "./lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib",
    "test": "nyc --reporter=text mocha --require @babel/register --exit --exclude test/app.js test",
    "docs": "jsdoc src --recurse --readme ./README.md --destination ./docs -c ./.jsdoc.conf"
  },
  "files": [
    "lib",
    "src"
  ],
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.5",
    "@babel/node": "7.14.5",
    "@babel/preset-env": "7.14.5",
    "@babel/register": "7.14.5",
    "chai": "4.3.4",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.23.4",
    "jsdoc": "3.6.7",
    "mocha": "9.0.0",
    "nyc": "15.1.0"
  },
  "dependencies": {
    "amqplib": "0.8.0",
    "eslint": "7.28.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drgroot/nodeMQ"
  },
  "keywords": [
    "rabbitmq",
    "amqplib",
    "AMQP"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drgroot/nodeMQ/issues"
  },
  "nyc": {
    "exclude": [
      "test/*.js"
    ]
  },
  "homepage": "https://github.com/drgroot/nodeMQ",
  "author": "Yusuf Ali <ali@yusuf.email> (https://yusufali.ca)"
}